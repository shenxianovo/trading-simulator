{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Order",
  "type": "object",
  "required": [
    "clOrderId",
    "market",
    "securityId",
    "side",
    "qty",
    "price",
    "shareholderId"
  ],
  "properties": {
    "clOrderId": {
      "type": "string",
      "maxLength": 16
    },
    "market": {
      "type": "string",
      "enum": ["XSHG", "XSHE", "BJSE"]
    },
    "securityId": {
      "type": "string",
      "maxLength": 6
    },
    "side": {
      "type": "string",
      "enum": ["B", "S"]
    },
    "qty": {
      "type": "integer",
      "minimum": 1
    },
    "price": {
      "type": "number",
      "minimum": 0
    },
    "shareholderId": {
      "type": "string",
      "maxLength": 10
    }
  }
}
