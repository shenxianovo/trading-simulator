{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "MatchRequest",
  "type": "object",
  "required": ["market", "securityId", "buyOrders", "sellOrders"],
  "properties": {
    "market": {
      "type": "string",
      "enum": ["XSHG", "XSHE", "BJSE"]
    },
    "securityId": {
      "type": "string"
    },
    "buyOrders": {
      "type": "array",
      "items": { "$ref": "./order.schema.json" },
      "description": "Buy side orders (price desc, time asc recommended)"
    },
    "sellOrders": {
      "type": "array",
      "items": { "$ref": "./order.schema.json" },
      "description": "Sell side orders (price asc, time asc recommended)"
    }
  }
}
