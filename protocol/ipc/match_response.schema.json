{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "MatchResponse",
  "type": "object",
  "required": ["trades", "remainingBuyOrders", "remainingSellOrders"],
  "properties": {
    "trades": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "tradeId",
          "buyOrderId",
          "sellOrderId",
          "price",
          "qty",
          "timestamp"
        ],
        "properties": {
          "tradeId": { "type": "string" },
          "buyOrderId": { "type": "string" },
          "sellOrderId": { "type": "string" },
          "price": { "type": "number" },
          "qty": { "type": "integer" },
          "timestamp": { "type": "integer" }
        }
      }
    },
    "remainingBuyOrders": {
      "type": "array",
      "items": { "$ref": "./order.schema.json" }
    },
    "remainingSellOrders": {
      "type": "array",
      "items": { "$ref": "./order.schema.json" }
    }
  }
}
