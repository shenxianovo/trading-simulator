{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "CancelRequest",
  "type": "object",
  "required": [
    "clOrderId",
    "origClOrderId",
    "market",
    "securityId",
    "shareholderId",
    "side"
  ],
  "properties": {
    "clOrderId": {
      "type": "string",
      "maxLength": 16
    },
    "origClOrderId": {
      "type": "string",
      "maxLength": 16
    },
    "market": {
      "type": "string",
      "enum": ["XSHG", "XSHE", "BJSE"]
    },
    "securityId": {
      "type": "string",
      "maxLength": 6
    },
    "shareholderId": {
      "type": "string",
      "maxLength": 10
    },
    "side": {
      "type": "string",
      "enum": ["B", "S"]
    }
  }
}
